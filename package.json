{
  "name": "solid-site",
  "license": "MIT",
  "scripts": {
    "build": "run-s format type:check build:vite build:postoptimize",
    "preview": "vite preview",
    "dev": "vite",
    "build:vite": "vite build",
    "build:postoptimize": "node ./postoptimize.mjs",
    "type:check": "tsc --noEmit",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\"",
    "fetch:releases": "jiti scripts/fetchReleases",
    "fetch:tutorials": "jiti scripts/fetchTutorials",
    "deploy:dev": "wrangler publish --env development",
    "deploy:prod": "wrangler publish --env production",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@amoutonbrady/solid-heroicons": "^0.9.0-rc.0",
    "@solid-primitives/intersection-observer": "^0.0.107",
    "@solid-primitives/throttle": "^1.0.0",
    "fuse.js": "^6.4.6",
    "markdown-it-prism": "^2.1.7",
    "monaco-editor": "^0.25.2",
    "prism-themes": "^1.7.0",
    "prismjs": "^1.24.0",
    "solid-app-router": "^0.0.51",
    "solid-js": "^1.0.3",
    "solid-meta": "^0.27.0",
    "solid-repl": "^0.14.0",
    "solid-utils": "^0.8.1",
    "vite-plugin-pwa": "^0.8.1"
  },
  "devDependencies": {
    "@aminya/minijson": "^0.6.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/markdown-it": "^12.0.2",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "dotenv": "^10.0.0",
    "fast-glob": "^3.2.6",
    "front-matter": "^4.0.2",
    "globby": "^11.0.4",
    "got": "^11.8.2",
    "htmlnano": "^1.0.0",
    "jiti": "^1.10.1",
    "markdown-it": "^12.1.0",
    "markdown-it-anchor": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "shiki": "^0.9.5",
    "svgo": "^2.3.1",
    "tailwindcss": "^2.2.4",
    "terser-config-atomic": "^0.1.1",
    "typescript": "^4.3.5",
    "vite": "^2.4.3",
    "vite-plugin-solid": "^2.0.1"
  }
}
